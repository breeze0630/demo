<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="xt.lb.dao.UserCarMapper">
     <resultMap id="BaseResultMap" type="xt.lb.entity.UserCar">
		<result column="user_id" property="userId" jdbcType="VARCHAR" />
		<result column="shop_id" property="shopId" jdbcType="VARCHAR" />
		<result column="goods_id" property="goodsId" jdbcType="VARCHAR" />
		<result column="amount" property="amount" jdbcType="INTEGER" />
    </resultMap>
    <sql id="Base_Column_List">
		userId, shopId, goodsId,amount
	</sql>
    <select id="selectByUserId" resultMap="BaseResultMap" parameterType="xt.lb.entity.UserCar">
        SELECT 
        <include refid="Base_Column_List"/>
        FROM user_car
        WHERE user_id = #{userId,jdbcType=VARCHAR}
    </select>
    
    
    </mapper>